    "@babel/preset-typescript": "^7.21.5",
    "@babel/runtime": "^7.21.5",
    "@reduxjs/toolkit": "^1.9.5",
    "@types/chrome": "^0.0.236",
    "@types/jest": "^29.5.1",
    "@types/react": "^18.2.6",
