{
  "name": "nerdefocus-chrome",
  "version": "0.5.4",
  "description": "Quickly debug focus when testing accessibility",
  "scripts": {
    "build": "npm version patch && webpack --config webpack.config.ts --mode=production && zip -r -X -j nerdefocus.zip build",
    "dev": "webpack --config webpack.config.ts --mode=development",
    "watch": "webpack watch --config webpack.config.ts --mode=development",
    "test": "jest",
    "savefont": "make fontsave -f fontello.mk",
    "openfont": "make fontopen -f fontello.mk"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wizzyfx/nerdeFocusPlugIn.git"
