{
  "name": "nerdefocus-chrome",
  "version": "0.5.6",
  "description": "Quickly debug focus when testing accessibility",
  "scripts": {
    "build": "npm version patch && webpack --config webpack.config.ts --mode=production && zip -r -X -j nerdefocus.zip build",
    "dev": "webpack --config webpack.config.ts --mode=development",
    "watch": "webpack watch --config webpack.config.ts --mode=development",
    "test": "jest",
    "savefont": "make fontsave -f fontello.mk",
    "openfont": "make fontopen -f fontello.mk"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wizzyfx/nerdeFocusPlugIn.git"
  },
  "author": "Ugi Kutluoglu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wizzyfx/nerdeFocusPlugIn/issues"
  },
  "homepage": "https://github.com/wizzyfx/nerdeFocusPlugIn#readme",
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-typescript": "^7.21.5",
    "@babel/runtime": "^7.21.5",
    "@types/chrome": "^0.0.236",
    "@types/jest": "^29.5.1",
    "@types/webpack": "^5.28.1",
    "@types/webpack-dev-server": "^4.7.2",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "babel-loader": "^9.1.2",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "esbuild-loader": "^4.3.0",
    "eslint": "^8.40.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "prettier": "^2.8.8",
    "sanitize.css": "^13.0.0",
    "style-loader": "^3.3.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "webpack": "^5.82.1",
    "webpack-cli": "^5.1.1",
    "webpack-dev-server": "^4.15.0"
  }
}
